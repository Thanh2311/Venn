<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Venn.FXController">
   <children>
      <BorderPane layoutX="3.0" layoutY="3.0" prefHeight="600.0" prefWidth="800.0">
         <center>
            <StackPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="269.0" prefWidth="392.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox maxWidth="-Infinity" minWidth="-Infinity" pickOnBounds="false" prefHeight="273.0" prefWidth="252.0" StackPane.alignment="CENTER_LEFT">
                     <children>
                        <Label fx:id="leftLabel" alignment="TOP_RIGHT" text="Left Set">
                           <font>
                              <Font name="System Bold" size="15.0" />
                           </font>
                        </Label>
                        <StackPane onMouseClicked="#selectSet" prefHeight="150.0" prefWidth="200.0">
                           <children>
                              <Circle fill="#e35b5b66" radius="126.0" stroke="BLACK" strokeType="INSIDE" />
                              <VBox fx:id="newLabelPaneLeft" maxHeight="-Infinity" maxWidth="-Infinity" pickOnBounds="false" />
                           </children>
                        </StackPane>
                     </children>
                  </VBox>
                  <VBox alignment="TOP_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity" pickOnBounds="false" StackPane.alignment="CENTER_RIGHT">
                     <children>
                        <Label fx:id="rightLabel" alignment="TOP_RIGHT" text="Right Set">
                           <font>
                              <Font name="System Bold" size="15.0" />
                           </font>
                        </Label>
                        <StackPane onContextMenuRequested="#context" onMouseClicked="#selectSet" prefHeight="150.0" prefWidth="200.0">
                           <children>
                              <Circle fill="#2197ff66" radius="126.0" stroke="BLACK" strokeType="INSIDE" />
                              <VBox fx:id="newLabelPaneRight" alignment="TOP_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity" pickOnBounds="false" />
                           </children>
                        </StackPane>
                     </children>
                  </VBox>
               </children>
            </StackPane>
         </center>
         <left>
            <VBox prefHeight="600.0" prefWidth="117.0" spacing="5.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="25.0" prefWidth="148.0">
                     <children>
                        <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#addEleButton" text="Add" />
                        <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="61.0" text="Rename" />
                     </children>
                  </HBox>
                  <TextField fx:id="textbox" promptText="Type Name Here" />
               </children>
               <padding>
                  <Insets left="5.0" top="50.0" />
               </padding>
            </VBox>
         </left>
      </BorderPane>
   </children>
</AnchorPane>
